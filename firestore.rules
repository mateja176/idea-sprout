rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /{document=**} {
      allow read, write: if false;
    }
    match /ideas/{ideaId} {
      allow read: if request.auth != null && (resource.data.author == request.auth.uid || resource.data.status == 'sprout');
    }
  }
}
